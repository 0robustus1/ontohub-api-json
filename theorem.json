{
  "id": "https://masterthesis.rightsrestricted.com/ontohub/theorem.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Theorem",
  "description": "A theorem inside of an ontology",
  "type": "object",
  "properties": {
    "iri": {
      "description": "The unique identifier for the theorem",
      "type": "string",
      "format": "uri"
    },
    "name": {
      "description": "The name of the theorem â€” usually auto-generated.",
      "type": "string"
    },
    "definition": {
      "description": "The definition of the theorem",
      "type": "string"
    },
    "evaluation_state": {
      "description": "The current state of the proof evaluation",
      "type": "string",
      "enum": [ "done", "failed", "pending", "processing" ]
    },
    "proof_status": { "$ref": "https://masterthesis.rightsrestricted.com/ontohub/proof_status.json#" },
    "ontology": {
      "$ref": "references.json#/definitions/ontology"
    },
    "symbols": {
      "description": "Reference to all symbols used in this theorem",
      "type": "string",
      "format": "uri"
    },
    "proof_attempts": {
      "description": "Reference to all proof attempts for this theorem",
      "type": "string",
      "format": "uri"
    }
  },
  "required": [
    "iri",
    "name",
    "definition",
    "evaluation_state",
    "symbols",
    "proof_attempts"
  ]
}
